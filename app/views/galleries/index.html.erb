z<h1>Gallery Results</h1>
<ul>
  <% for gallery in @galleries %>
      <li><b><%= gallery.name %> (<i><%= gallery.photographer.name %></i>)</b>
        <ul>
          <% for photo in gallery.photos %>
              <li><%= photo.name %> (<%= photo.file_path %>)</li>
          <% end %>
        </ul>
      </li>
  <% end %>
</ul>
<select name="role">
  <% for role in @roles %>
      <option value="<%= role.id %>"><%= role.role_name %></option>
  <% end %>
</select>
